[
    { "text": "Серые глаза - рассвет," },
    { "text": "Пароходная сирена," },
    { "text": "Дождь, разлука, серый след" },
    { "text": "За винтом бегущей пены." },
    { "text": "Черные глаза - жара," },
    { "text": "В море сонных звезд скольженье," },
    { "text": "И у борта до утра" },
    { "text": "Поцелуев отраженье." },
    { "text": "Синие глаза - луна," },
    { "text": "Вальса белое молчанье," },
    { "text": "Ежедневная стена" },
    { "text": "Неизбежного прощанья." },
    { "text": "Карие глаза - песок," },
    { "text": "Осень, волчья степь, охота," },
    { "text": "Скачка, вся на волосок" },
    { "text": "От паденья и полета." },
    { "text": "Нет, я не судья для них," },
    { "text": "Просто без суждений вздорных" },
    { "text": "Я четырежды должник" },
    { "text": "Синих, серых, карих, черных." },
    { "text": "Как четыре стороны" },
    { "text": "Одного того же света," },
    { "text": "Я люблю - в том нет вины -" },
    { "text": "Все четыре этих цвета." }
]